<table border='1'>
  <thead>
   <tr>
    <td>S.No</td>
    <td>Username</td>
    <td>Colors Placed</td>
    <td>Favourite Color</td>
   </tr>
  </thead>
  <tbody>
   <%@users_data.each_with_index do |user,index|%>
    <tr>
     <td><%= index+=1%>.</td>
     <td><%= user.name%></td>
     <td><%= user.most_color%></td>
     <td style="background-color: <%= user.user_favourite_color%>"></td>
    </tr>
   <%end%>
  </tbody>
</table>